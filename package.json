{
  "name": "aws-masking",
  "displayName": "AWS Masking",
  "version": "0.1.4",
  "description": "Enhance your security by auto-hiding sensitive information on AWS Management Console.",
  "author": "koki-develop <kou.pg.0131@gmail.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "fmt": "prettier --write ."
  },
  "dependencies": {
    "@headlessui/react": "1.7.19",
    "@plasmohq/storage": "1.15.0",
    "clsx": "2.1.1",
    "plasmo": "0.85.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.5.1",
    "@parcel/optimizer-data-url": "2.9.3",
    "@parcel/transformer-inline-string": "2.9.3",
    "@types/chrome": "0.1.0",
    "@types/node": "20.19.8",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.21",
    "postcss": "8.5.6",
    "prettier": "3.2.4",
    "tailwindcss": "3.4.17",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*.console.aws.amazon.com/*",
      "https://*.awsapps.com/*",
      "https://*.signin.aws.amazon.com/*",
      "https://health.aws.amazon.com/*"
    ]
  }
}
